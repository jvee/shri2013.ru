<div class="b-person__name">{{=it.name}}</div>
{{?it.photo}}
<img class="b-person__photo" src="{{=it.photo}}" alt="{{=it.name}}">
{{?}}
<div class="b-person__about">{{= it.about}}</div>
{{?it.profiles}}
<ul class="b-person__profile-list">
	{{~it.profiles :profile:index}}
		<li class="b-person__profile-item"><a href="{{=profile.link}}">{{=profile.name}}</a></li>
	{{~}}
</ul>
{{?}}